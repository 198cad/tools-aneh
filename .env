# TOOLS_POSTGRES_PORT=5432
# TOOLS_POSTGRES_USER=postgres
# TOOLS_POSTGRES_PASSWORD=postgres
# TOOLS_POSTGRES_DATABASE=postgres



# RabbitMQ Configuration
RABBITMQ_HOST=10.8.0.1
RABBITMQ_MANAGEMENT_PORT=15672
RABBITMQ_DEFAULT_USER=tampan_wuzapi
RABBITMQ_DEFAULT_PASS=Nuf@ayukwulandar1
RABBITMQ_DEFAULT_VHOST=wuzapi

# Alternative RabbitMQ (using TOOLS_ prefix)
